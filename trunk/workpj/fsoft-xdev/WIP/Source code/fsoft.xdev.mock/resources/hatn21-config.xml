    		<result name="update" >/pages/list/TrustRegionList.jsp</result>  
    		<result name="input">/pages/input/TrustRegionInput.jsp</result>    		    	   		     		
    	</action> 
    				
    	<action name="*TrustDistrict" method ="{1}" class="trustDistrictAction">
    		<result name="success">/pages/embed/TrustDistrictList.jsp</result>   		
    		<result name="list" type="json">
    			<param name="excludeProperties">trustDistrict</param>
    		</result>     		  
    		<result name="add" ></result>
    		<result name="update" >/pages/input/TrustDistrictInput.jsp</result>  
    		<result name="input">/pages/input/TrustDistrictInput.jsp</result>      		 		
    	</action>
    			  	   		             	
    	<action name="*GovOfficeRegion" method ="{1}" class="govOfficeRegionAction">
    		<result name="list" type="json">
    			<param name="excludeProperties">govOfficeRegion</param>
    		</result> 
    		<result name="input">/pages/input/govOfficeInput.jsp</result> 
    		    		
    	</action>     	
    	  
    	
    	<action name="*Team" method ="{1}" class="teamAction">    		
    		<result name="list" type="json">
    			<param name="excludeProperties">team</param>
    		</result> 
    		<result name="add" ></result>
    		<result name="update" ></result>  
    		<result name="input">/pages/input/teamInput.jsp</result>    		    	   		     		
    			
    	</action>  
    	
    	<action name="*Directorate" method="{1}" class="directorateAction">    											
    		<result name="success">/pages/embed/directorateList.jsp</result>
    		<result name="list" type="json">
    			<param name="excludeProperties">directorate,directoratesDao</param>
    		</result>
    		<result name="add" ></result>
    		<result name="update" ></result>  
    		<result name="input">/pages/input/directorateInput.jsp</result>  
    	</action>
    	
    </package>
</struts>
