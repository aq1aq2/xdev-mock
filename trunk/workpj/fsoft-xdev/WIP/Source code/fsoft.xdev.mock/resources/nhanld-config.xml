<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
    <!-- Configuration Information Created by NhanLD -->
    <package name="nhanld" extends="struts-default,json-default">
    
    	<action name="*Nav" method="{1}" class="fsoft.xdev.mock.actions.NavigateAction">
    		<result name="home">/pages/Home.jsp</result>
    		<result name="org">/pages/list/organisationList.jsp</result>
    		<result name="service">/pages/list/serviceList.jsp</result>
    		<result name="programme">/pages/list/programmeList.jsp</result>
    		<result name="region">/pages/list/TrustRegionList.jsp</result>
    		<result name="gov">/pages/list/govOfficeList.jsp</result>
    		<result name="premise">/pages/list/premiseList.jsp</result>
    	</action>
    	 
    	<action name="*Login" method="{1}" class="loginAction">
    		<result name="in">/pages/Home.jsp</result>
    		<result name="out">/pages/Login.jsp</result>
    		<result name="input">/pages/Login.jsp</result>
    		<result name="wrong">/pages/Login.jsp</result>
    	</action>
    
    	<action name="*Contact" method="{1}" class="contactAction">
    		<result name="success" >/pages/lookup/contactList.jsp</result>
    		
    		<result name="list" type="json">
    			<param name="excludeProperties">contact,listContactType,listBestContactMethod</param>
    		</result>
    		<result name="input">/pages/input/contactInput.jsp</result>
    		<result name="save" type="json"></result> 
    	</action>
    	
    	<action name="*TypeOfBusiness" method="{1}" class="typeOfBusinessAction">
    		<result name="success" >/pages/lookup/typeOfBusinessLookup.jsp</result>
    		
    		<result name="list" type="json">
    			<param name="excludeProperties">typeOfBusinessDao</param>
    		</result>
    	</action>
    	
    	<action name="*Address" method="{1}" class="addressAction">
    		<result name="success" >/pages/lookup/addressLookup.jsp</result>
    		
    		<result name="list" type="json">
    			<param name="excludeProperties">addressDao</param>
    		</result>
    	</action>
    	
   	</package>
</struts>