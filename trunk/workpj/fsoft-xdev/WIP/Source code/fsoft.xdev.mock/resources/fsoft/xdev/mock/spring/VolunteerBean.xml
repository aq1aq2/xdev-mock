<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">


	<bean id="volunteerAction" class="fsoft.xdev.mock.actions.volunteering.VolunteeringAction">
		<property name="volunteerDao" ref="volunteerDao" />
	</bean>

	<bean id="volunteerDao" parent="txProxyTemplate">		
		<property name="target">
    		<bean class="fsoft.xdev.mock.dao.imp.VolunteerDao">
        		<property name="sessionFactory" ref="sessionFactory" />
        	</bean>
        </property>
	</bean>    
</beans>