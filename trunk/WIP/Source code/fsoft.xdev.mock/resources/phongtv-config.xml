<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<!-- Configuration Information Created by DungNM2 -->
	<package name="phongtv" extends="json-default">
		<action name="*Premises" method="{1}" class="premisesAction">
			<result name="list" type="json"></result>
			<result name="add">/pages/list/premiseList.jsp</result>
			<result name="detail">/pages/input/premiseInput.jsp</result>
		</action>
		<action name="*Facilities" method="{1}" class="facilitiesAction">
			<result name="list" type="json">
				<param name="excludeProperties">facility,listFacilitiesType,listConnectivityType </param>
			</result>
			<result name="add">/pages/list/facilitiesList.jsp</result>
			<result name="detail">/pages/input/facilitiesInput.jsp</result>
		</action>
		
		<action name="*VolunteeringOpportunity" method="{1}" class="volunteerAction">
			<result name="list" type="json">
				<param name="excludeProperties">volunteer</param>
			</result>
			<result name="add">/pages/list/volunteeringOpportunityList.jsp</result>
			<result name="detail">/pages/input/volunteerInput.jsp</result>
		</action>
		
		<action name="view"  class="facilitiesAction" method="list">
			<result name="list">/pages/list/test.jsp</result>
			<result name="add"></result>
		</action>
		
	</package>
	
</struts>