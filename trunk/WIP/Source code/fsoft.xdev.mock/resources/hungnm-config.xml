<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<!-- Configuration Information Created by HungNM -->
	<package name="hungnm" extends="json-default">
		<action name="*Service" method="{1}" class="serviceAction">
			<result name="list" type="json">
			<param name="excludeProperties">listServiceType,listSubType,referenceDataDao,servicesDao,service,listServiceBenefitsCriterion,listServiceBarriersCriterion,listServiceEthnicityCriterion,listServiceDisabilityCriterion,listServicePersonalCircumstancesCriterion,listOtherServiceParticipationCriterion,
							listClientSupportProcess,listIntervention,listClientJourney,listOtherServices,
							listSupportCentres,listClientOutcome,listTargetClient,listAccreditations,
							listReferralSources,listProgramme,listContactOutcome,listContactObligation,
							listParticipation,listServiceBenefitsCriterion_Checked,listServiceBarriersCriterion_Checked,
							listServiceEthnicityCriterion_Checked
				</param>
			</result>
			<result name="detailService">/pages/input/serviceInput.jsp</result>
			<result name="update">/pages/input/serviceInput.jsp</result>
			<result name="save"></result>
	
		</action>
		
		<action name="*Programme" method="{1}" class="programmeAction">
			<result name="list" type="json"></result>
			<result name="save" type="json"></result>
			
		</action>
	</package>
</struts>