<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<!-- Configuration Information Created by HungNM -->
	<package name="hungnm" extends="json-default">
		<action name="*Service" method="{1}" class="serviceAction">
			<result name="list" type="json"></result>
		</action>
		
		<action name="*Programme" method="{1}" class="programmeAction">
			<result name="list" type="json"></result>
		</action>
	</package>
</struts>