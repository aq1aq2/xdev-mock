<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

			  
	<bean id="supportingMaterialAction" class="fsoft.xdev.mock.actions.material.SupportingMaterialsAction">
		<property name="entityDao" ref="entityDao" />
	</bean>

	<bean id="entityDao" parent="txProxyTemplate">		
		<property name="target">
    		<bean class="fsoft.xdev.mock.dao.imp.SupportingMaterialDao">
        		<property name="sessionFactory" ref="sessionFactory" />
        	</bean>
        </property>
	</bean>    

</beans>