<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">
	<bean id="serviceAction" class="fsoft.xdev.mock.actions.services.ServiceAction">
		<property name="servicesDao" ref="servicesDao"/>
	</bean>
	<!-- initial -->
	<bean id="servicesDao" parent="txProxyTemplate">
		<property name="target">
			<bean class="fsoft.xdev.mock.dao.imp.ServicesDao">
				<property name="sessionFactory" ref="sessionFactory"/>
			</bean>
		</property>
	</bean>
</beans>
