<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
    <!-- Configuration Information Created by DungNM2 -->
    <package name="dungnm2" extends="struts-default,json-default">    				
		<action name="*Organisation" method="{1}" class="organisationAction">
    		<result name="list" type="json">
    			<param name="excludeProperties">referenceDataDao,organisation,organisationDao,listOrgSpecicalism,listServicePerCirCap,listServiceDisCap,listServiceEthCap,listServiceBarCap,listAccreditation,listServiceBenCap,listEoiprogramme,listEoiservice</param>
    		</result>
 			<result name="success">/pages/lookup/organisationLookup.jsp</result>
 			<result name="input">/pages/input/organisationInput.jsp</result>
    		<result name="save" type="json"></result>
    		<result name="update" type="json"></result>
    	</action>    	
    	<action name="*SupportingMaterial" method="{1}" class="supportingMaterialAction">
    		<result name="success">/pages/embed/supportingMaterialList.jsp</result>
    		<result name="list" type="json">
    			<param name="excludeProperties">entityDao,entity</param>
    		</result>
    		<result name="input">/pages/input/supportingMaterialInput.jsp</result>
    		<result name="save" type="json">
    			<param name="excludeProperties">entity,listModel,entityDao</param>
    		</result>
    	</action>
    	<action name="*Department" method="{1}" class="departmentAction">
    		<result name="list" type="json"></result>
    		<result name="save" type="json"></result>
    	</action>
	</package>
</struts>