<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
           xmlns:dis="http://www.dis-corp.com" elementFormDefault="qualified"
           targetNamespace="http://www.dis-corp.com">

  <xs:attribute name="id" type="xs:string">
    <xs:annotation>
      <xs:documentation>Represents a unique identifier as assigned by a business system</xs:documentation>
    </xs:annotation>
  </xs:attribute>


  <xs:complexType name="identifierReference">
    <xs:attribute name="webId" type="xs:integer" use="optional">
      <xs:annotation>
        <xs:documentation>
          The is the primary key of the record on the middle tier. This field should have a value when
          communicating from the middle tier to the mobile device/web
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="internalId" type="xs:string" use="optional">
      <xs:annotation>
        <xs:documentation>
          This is the primary identifier on the DMS system
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>


  <xs:simpleType name="dateTime">
    <xs:annotation>
      <xs:documentation>Type representing either a date or date with time</xs:documentation>
    </xs:annotation>
    <xs:union memberTypes="xs:date xs:dateTime"/>
  </xs:simpleType>

  <xs:element name="Dealer">
    <xs:complexType>
      <xs:sequence minOccurs="0" maxOccurs="1">
        <xs:element name="Name" type="xs:string"></xs:element>
        <xs:element ref="dis:Companies" minOccurs="0" maxOccurs="1"/>
        <!--
                <xs:element ref="dis:Branches" minOccurs="0" maxOccurs="1"></xs:element>
                <xs:element ref="dis:Departments" minOccurs="0" maxOccurs="1"></xs:element>
                -->
      </xs:sequence>
      <xs:attribute name="webId" type="xs:string" use="optional">
        <xs:annotation>
          <xs:documentation>
            The is the primary key of the record on the middle tier.
          </xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="enterpriseId" use="optional" type="xs:string">
        <xs:annotation>
          <xs:documentation>The dealers DIS enterprise ID.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
    </xs:complexType>
  </xs:element>

  <xs:element name="Companies">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="1" maxOccurs="unbounded" ref="dis:Company"></xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <xs:element name="Company">
    <xs:complexType>
      <xs:sequence maxOccurs="1" minOccurs="0">
        <xs:element name="Name" type="xs:string"/>
        <!-- <xs:element name="code" type="xs:string" minOccurs="0" maxOccurs="1"/> -->
        <!-- <xs:element name="internalId" type="xs:string" minOccurs="0" maxOccurs="1"/>  major -->
        <!-- <xs:element name="dealerId" type="xs:string" minOccurs="0"/> Major -->
        <!--- 	<xs:element name="version" type="xs:long"/>  major -->
        <xs:element ref="dis:Branches" minOccurs="0" maxOccurs="1"/>
        <xs:element name="MainBranchId" type="dis:branchReference" minOccurs="0" maxOccurs="1"/>
        <xs:element name="FiscalStartMonth" type="xs:integer" minOccurs="0" maxOccurs="1"/>
      </xs:sequence>
      <xs:attribute name="internalId" use="optional" type="xs:string"/>
      <xs:attribute name="webId" type="xs:string" use="optional">
        <xs:annotation>
          <xs:documentation>
            The is the primary key of the record on the middle tier.
          </xs:documentation>
        </xs:annotation>
      </xs:attribute>
    </xs:complexType>
  </xs:element>

  <xs:element name="Branches">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="1" maxOccurs="1" name="Dealer" type="dis:dealerReference"/>
        <xs:element minOccurs="1" maxOccurs="1" name="Company" type="dis:companyReference"/>
        <xs:element minOccurs="1" maxOccurs="unbounded" ref="dis:Branch"></xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <xs:element name="Branch">
    <xs:complexType>
      <xs:sequence minOccurs="0" maxOccurs="1">
        <xs:element name="Name"></xs:element>
        <xs:element ref="dis:Contact" minOccurs="0"></xs:element>
        <xs:element name="Customer" type="dis:customerReference" minOccurs="0" maxOccurs="1"/>
        <!--  Major. Use Address reference instead of address type.
                <xs:element name="Address" type="dis:address"></xs:element>
                -->
        <xs:element ref="dis:Address" minOccurs="0" maxOccurs="unbounded"/>
        <xs:element ref="dis:Departments" minOccurs="0" maxOccurs="1"/>
      </xs:sequence>
      <xs:attribute name="internalId" use="optional" type="xs:string">
        <xs:annotation>
          <xs:documentation>
            DMS identifier for record
          </xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="webId" use="optional" type="xs:string">
        <xs:annotation>
          <xs:documentation>
            The is the primary key of the record on the middle tier.
          </xs:documentation>
        </xs:annotation>
      </xs:attribute>
    </xs:complexType>
  </xs:element>


  <xs:element name="Departments">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="1" maxOccurs="unbounded" ref="dis:Department"></xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>

    <xs:simpleType name="departmentType">
        <xs:restriction base="xs:string">
            <xs:enumeration value="Undefined">
                <xs:annotation>
                    <xs:documentation>Department type is not defined. Value = 0.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="Parts">
                <xs:annotation>
                    <xs:documentation>Department type is Parts. Value = 1.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="Service">
                <xs:annotation>
                    <xs:documentation>Department type is Service. Value = 2.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="UnitSales">
                <xs:annotation>
                    <xs:documentation>Department type is Unit Sales. Value = 3.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="Rental">
                <xs:annotation>
                    <xs:documentation>Department type is Rental. Value = 4.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="Admin">
                <xs:annotation>
                    <xs:documentation>Department type is Admin. Value = 5.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
        </xs:restriction>
    </xs:simpleType>

    <xs:element name="Department">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="name" type="xs:string" minOccurs="1" maxOccurs="1"></xs:element>
        <xs:element name="type" type="dis:departmentType" minOccurs="0" maxOccurs="1"></xs:element>
      </xs:sequence>
      <xs:attribute name="internalId" use="optional" type="xs:string">
        <xs:annotation>
          <xs:documentation>
            DMS identifier for record
          </xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="webId" use="optional" type="xs:string">
        <xs:annotation>
          <xs:documentation>
            The is the primary key of the record on the middle tier.
          </xs:documentation>
        </xs:annotation>
      </xs:attribute>
    </xs:complexType>
  </xs:element>


  <xs:complexType name="departmentReference">
    <xs:sequence minOccurs="0" maxOccurs="1">
      <xs:element name="name" type="xs:string"></xs:element>
    </xs:sequence>
    <xs:attribute name="internalId" use="optional" type="xs:string">
      <xs:annotation>
        <xs:documentation>
          DMS system identifier for record
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="webId" use="optional" type="xs:string">
      <xs:annotation>
        <xs:documentation>
          The is the primary key of the record on the middle tier.
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>


  <xs:element name="Customer">
    <xs:complexType>
      <xs:sequence minOccurs="0" maxOccurs="1">
        <xs:element name="Name" type="xs:string"></xs:element>
        <xs:element name="Branch" type="dis:branchReference" minOccurs="0" maxOccurs="1">
          <xs:annotation>
            <xs:documentation>
              Used by Keystone to denote which branch a customer belongs to.
            </xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="BillTo" type="dis:addressType" minOccurs="0"></xs:element>
        <xs:element name="ShipToAddresses" minOccurs="0">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="ShipTo" type="dis:addressType" minOccurs="0" maxOccurs="unbounded"></xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element ref="dis:SolvencyCode" minOccurs="0" maxOccurs="1"/>
        <xs:element ref="dis:CurrencyCode" minOccurs="0" maxOccurs="1"/>
        <xs:element ref="dis:ChargeCode" minOccurs="0" maxOccurs="1"/>
        <xs:element ref="dis:CommodityCode" minOccurs="0"/>
        <xs:element ref="dis:DiscountCode" minOccurs="0" maxOccurs="1"/>
        <xs:element ref="dis:PriceCode" minOccurs="0" maxOccurs="1"/>
        <xs:element name="DefaultOrderAddress" type="dis:addressReference" minOccurs="1" maxOccurs="1"/>
        <xs:element name="DefaultShipToCustomer" type="dis:customerReference" minOccurs="1" maxOccurs="1"/>
        <xs:element name="DefaultShipToAddress" type="dis:addressReference" minOccurs="1" maxOccurs="1"/>
        <xs:element name="DefaultBillToCustomer" type="dis:customerReference" minOccurs="1" maxOccurs="1"/>
        <xs:element name="DefaultBillToAddress" type="dis:addressReference" minOccurs="1" maxOccurs="1"/>
        <xs:element name="DefaultPaymentCustomer" type="dis:customerReference" minOccurs="1" maxOccurs="1"/>
        <xs:element name="DefaultPaymentAddress" type="dis:addressReference" minOccurs="1" maxOccurs="1"/>
        <xs:element name="PurchaseOrderRequired" type="xs:boolean" minOccurs="0" maxOccurs="1"/>
        <xs:element name="Active" type="xs:boolean" minOccurs="0" maxOccurs="1"/>
        <xs:element name="DamageWaiver" type="xs:boolean" minOccurs="0" maxOccurs="1"/>
        <xs:element ref="dis:LegalText" minOccurs="0" maxOccurs="1"/>
        <xs:element ref="dis:Territory" minOccurs="0" maxOccurs="1"/>
        <xs:element name="Language" type="xs:language" minOccurs="0" maxOccurs="1"/>
        <xs:element name="GroupInvoices" type="xs:integer" minOccurs="0" maxOccurs="1"/>
        <xs:element name="SalesPersons" minOccurs="0" maxOccurs="1">
          <xs:complexType>
            <xs:sequence>
              <xs:element maxOccurs="unbounded" name="SalesPerson" type="dis:userReference" minOccurs="0"/>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="Addresses" minOccurs="0" maxOccurs="1">
          <xs:complexType>
            <xs:sequence>
              <xs:element ref="dis:Address" maxOccurs="unbounded"/>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="Contacts" minOccurs="0" maxOccurs="1">
          <xs:complexType>
            <xs:sequence>
              <xs:element ref="dis:Contact" maxOccurs="unbounded"/>
            </xs:sequence>
          </xs:complexType>
        </xs:element>

      </xs:sequence>
      <xs:attribute name="internalId" use="optional" type="xs:string">
        <xs:annotation>
          <xs:documentation>
            DMS system identifier for record
          </xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="webId" use="optional" type="xs:string">
        <xs:annotation>
          <xs:documentation>
            The is the primary key of the record on the middle tier.
          </xs:documentation>
        </xs:annotation>
      </xs:attribute>
    </xs:complexType>
  </xs:element>

  <xs:simpleType name="communicationMethodType">
    <xs:restriction base="xs:string">
      <xs:enumeration value="PHONE">
        <xs:annotation>
          <xs:documentation>Phone number</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="FAX">
        <xs:annotation>
          <xs:documentation>Fax number</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="EMAIL">
        <xs:annotation>
          <xs:documentation>Email address</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="EM_INV">
        <xs:annotation>
          <xs:documentation>Email for invoice</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="CELL">
        <xs:annotation>
          <xs:documentation>Cellular/Mobile phone</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="FAX_INV">
        <xs:annotation>
          <xs:documentation>FAX for invoices</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="TWITTER">
        <xs:annotation>
          <xs:documentation>Twitter account</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="FACEBOOK">
        <xs:annotation>
          <xs:documentation>Facebook account</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="WWW">
        <xs:annotation>
          <xs:documentation>Website address</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>

  <xs:element name="CommunicationDetail">
    <xs:annotation>
      <xs:documentation>
        This is the different means of communication that any entity in a business system can have
        (Phone, Fax, Email...).
      </xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element name="Information" type="xs:string"/>
        <xs:element name="Main" type="xs:boolean"/>
        <xs:element name="Extension" minOccurs="0" maxOccurs="1"/>
      </xs:sequence>
      <xs:attribute name="method" type="dis:communicationMethodType" use="required">
        <xs:annotation>
          <xs:documentation>The communcation method id.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="internalId" use="optional" type="xs:string">
        <xs:annotation>
          <xs:documentation>
            This is the id for a manufacturer that is used by the DMS.
          </xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="webId" use="optional" type="xs:string">
        <xs:annotation>
          <xs:documentation>
            The is the primary key of the record on the middle tier.
          </xs:documentation>
        </xs:annotation>
      </xs:attribute>
    </xs:complexType>
  </xs:element>


  <xs:element name="Contact">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="Title" type="xs:string" minOccurs="0" maxOccurs="1"/>
        <xs:element name="FirstName" type="xs:string" minOccurs="0" maxOccurs="1"/>
        <xs:element name="MiddleName" type="xs:string" minOccurs="0" maxOccurs="1"/>
        <xs:element name="LastName" type="xs:string" minOccurs="1" maxOccurs="1"/>
        <xs:element name="Suffix" type="xs:string" minOccurs="0" maxOccurs="1"/>
        <xs:element name="NickName" type="xs:string" minOccurs="0" maxOccurs="1"/>
        <xs:element name="Gender" type="xs:string" minOccurs="1" maxOccurs="1">
          <xs:annotation>
            <xs:documentation>
              M for Male
              F for Female
            </xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="Language" type="xs:language" minOccurs="1" maxOccurs="1"/>
        <xs:element name="Role" type="dis:identifierReference" minOccurs="0" maxOccurs="1"/>
        <xs:element name="Relationship" minOccurs="0" maxOccurs="1">
          <xs:complexType>
            <xs:attribute name="id" type="dis:relationship" use="optional"/>
          </xs:complexType>
        </xs:element>
        <xs:element name="BirthDay" type="xs:dateTime" minOccurs="0" maxOccurs="1"/>
        <xs:element name="Notes" type="xs:string" minOccurs="0" maxOccurs="1"/>
        <!--<xs:element ref="dis:Address" name="Address" type="dis:identifierReference" />-->
        <xs:element ref="dis:Address" minOccurs="0" maxOccurs="1"/>
        <xs:element name="MasterContact" type="dis:contactReference" minOccurs="0" maxOccurs="1"/>
        <xs:element name="SalesPersons" minOccurs="0" maxOccurs="1">
          <xs:complexType>
            <xs:sequence>
              <xs:element maxOccurs="unbounded" name="SalesPerson" type="dis:userReference"/>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <!--
                <xs:element name="Addresses" minOccurs="0">
                  <xs:complexType>
                    <xs:sequence>
                      <xs:element ref="dis:Address" minOccurs="1" maxOccurs="unbounded"></xs:element>
                    </xs:sequence>
                  </xs:complexType>
                </xs:element>
                -->
      </xs:sequence>
      <xs:attribute name="internalId" use="optional" type="xs:string">
        <xs:annotation>
          <xs:documentation>
            DMS system identifier for record
          </xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="webId" use="optional" type="xs:string">
        <xs:annotation>
          <xs:documentation>
            The is the primary key of the record on the middle tier.
          </xs:documentation>
        </xs:annotation>
      </xs:attribute>
    </xs:complexType>
  </xs:element>

  <!--
      <xs:element name="Manufacturers">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="Manufacturer" type="dis:manufacturer" minOccurs="1" maxOccurs="unbounded"></xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
    -->
  <xs:complexType name="manufacturer">
    <xs:sequence>
      <xs:element name="Name" type="xs:string" minOccurs="0" maxOccurs="1"></xs:element>
      <xs:element name="DisplayName" type="xs:string" minOccurs="1" maxOccurs="1"/>
      <xs:element name="ProductType" type="dis:productType" minOccurs="1" maxOccurs="1"/>
      <xs:element name="Description" type="xs:string" minOccurs="0" maxOccurs="1"/>
    </xs:sequence>
    <xs:attribute name="internalId" use="optional" type="xs:string">
      <xs:annotation>
        <xs:documentation>
          This is the id for a manufacturer that is used by the DMS.

          For labor and miscellaneous products, labor and miscellaneous manufacturers need to be setup.
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="webId" use="optional" type="xs:string">
      <xs:annotation>
        <xs:documentation>
          The is the primary key of the record on the middle tier.
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>

  <xs:complexType name="manufacturerReference">
    <xs:attribute name="internalId" type="xs:string" use="optional">
      <xs:annotation>
        <xs:documentation>
          A commodity code is a code that is assigned by parts manufacturers so that parts can be grouped into
          categories. The commodity codes are used in determining the prices on parts
          Example:
          FF - Fuel Filters
          OF - Oil Filters
          AF - Air Filters
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="webId" type="xs:integer" use="optional"/>
  </xs:complexType>

  <xs:complexType name="manufacturerAcknowledgeReference">
    <xs:sequence>
      <xs:element ref="dis:AcknowledgeStatus" minOccurs="1" maxOccurs="1"/>
    </xs:sequence>
    <xs:attribute name="internalId" use="optional" type="xs:string">
      <xs:annotation>
        <xs:documentation>
          This is the id for a manufacturer that is used by the DMS.

          For labor and miscellaneous products, labor and miscellaneous manufacturers need to be setup.
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="webId" use="optional" type="xs:string">
      <xs:annotation>
        <xs:documentation>
          The is the primary key of the record on the middle tier.
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>

  <xs:element name="ShippingMethods">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="ShippingMethod" type="dis:shippingMethod" minOccurs="0" maxOccurs="unbounded"></xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <xs:complexType name="shippingMethod">
    <xs:sequence>
      <xs:element name="Name" type="xs:string"></xs:element>
      <xs:element name="Code" type="xs:string"></xs:element>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="machine">
    <xs:annotation>
      <xs:documentation>A piece of equipment.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:sequence minOccurs="0" maxOccurs="1">
        <xs:element name="Manufacturer" type="dis:manufacturerReference" minOccurs="0" maxOccurs="1"/>
        <xs:element ref="dis:Model" minOccurs="0" maxOccurs="1"/>
        <xs:element ref="dis:SerialNumber" minOccurs="0" maxOccurs="1"/>
      </xs:sequence>
      <xs:element minOccurs="0" ref="dis:EngineSerialNumber"/>
      <xs:element minOccurs="0" ref="dis:VIN"/>
      <xs:element minOccurs="0" ref="dis:AlternateID"/>
      <xs:sequence minOccurs="0">
        <xs:element ref="dis:HourMeter" minOccurs="0"/>
        <xs:element ref="dis:Odometer" minOccurs="0"/>
      </xs:sequence>
      <xs:element name="Location" minOccurs="0" type="dis:Location">
        <xs:annotation>
          <xs:documentation>The location of the equipment.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>

    <xs:attribute name="id" type="xs:string"/>
  </xs:complexType>

  <xs:complexType name="customerAddress">
    <xs:sequence>
      <xs:element name="Customer" type="dis:customerReference" minOccurs="0"/>
      <xs:element name="Address" type="dis:addressType"/>
    </xs:sequence>
  </xs:complexType>


  <xs:element name="Model" type="xs:string"/>
  <xs:element name="SerialNumber" type="xs:string"/>
  <xs:element name="EngineSerialNumber" type="xs:string"/>
  <xs:element name="VIN" type="xs:string"/>
  <xs:element name="AlternateID" type="xs:string"/>
  <xs:element name="HourMeter" type="dis:nonNegativeDecimal"/>
  <xs:element name="Odometer" type="dis:nonNegativeDecimal"/>

  <xs:complexType name="Location">
    <xs:annotation>
      <xs:documentation>
        A physical geographical location. Specified either via lat and long or a postal address.
      </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:sequence minOccurs="0">
        <xs:element ref="dis:Lattitude"/>
        <xs:element ref="dis:Longitude"/>
      </xs:sequence>
      <xs:element minOccurs="0" name="Address" type="dis:addressType"/>
    </xs:sequence>
  </xs:complexType>
  <xs:element name="Lattitude" type="xs:decimal"/>
  <xs:element name="Longitude" type="xs:decimal"/>

  <xs:complexType name="Location1">
    <xs:annotation>
      <xs:documentation>A physical geographical location. Specified either via lat and long</xs:documentation>
    </xs:annotation>
    <xs:sequence minOccurs="1" maxOccurs="1">
      <xs:element ref="dis:Lattitude"/>
      <xs:element ref="dis:Longitude"/>
    </xs:sequence>
  </xs:complexType>

  <xs:simpleType name="nonNegativeDecimal">
    <xs:annotation>
      <xs:documentation></xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:decimal">
      <xs:minInclusive value="0.0"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="email">
    <xs:annotation>
      <xs:documentation>Email Address</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:pattern value="([a-zA-Z0-9_\-])([a-zA-Z0-9_\-\.]*)@(\[((25[0-5]|2[0-4][0-9]|1[0-9][0-9]|[1-9][0-9]|[0-9])\.){3}|((([a-zA-Z0-9\-]+)\.)+))([a-zA-Z]{2,}|(25[0-5]|2[0-4][0-9]|1[0-9][0-9]|[1-9][0-9]|[0-9])\])"/>
    </xs:restriction>
  </xs:simpleType>

