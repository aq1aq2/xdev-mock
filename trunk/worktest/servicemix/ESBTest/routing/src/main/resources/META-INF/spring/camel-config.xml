<!--       <camel:to uri="cxf:bean:serviceA_endpoint" /> -->
<!--     </camel:route> -->
<!--   </camel:camelContext> -->
    
	<!-- Enterprise Services CXF Endpoint -->
	<cxf:cxfEndpoint id="enterpriseServiceEndpoint"
                     address="/webservices/enterprise"
                     endpointName="dis:EnterprisePort"
                     serviceName="dis:EnterpriseService"
                     wsdlURL="dn/servicemix/example/schemas/enterprise.wsdl"
                     xmlns:dis="http://www.dis-corp.com"/>
                     
	<!-- Remote CXF Endpoint                    
	<cxf:cxfEndpoint id="externalServiceEndpoint"
                     address="http://10.16.6.61:8181/cxf/webservices/2.0/enterprise"
                     wsdlURL="http://10.16.6.61:8181/cxf/webservices/2.0/enterprise?wsdl"
                     endpointName="dis:EnterpriseEndpoint"
                     serviceName="dis:EnterpriseService"
                     xmlns:dis="http://www.dis-corp.com"/>
                     -->
                                          
